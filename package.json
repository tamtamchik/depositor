{
  "name": "@tamtamchik/depositor",
  "version": "1.0.0",
  "description": "CLI tool for generating Ethereum 2.0 deposit_data and keystore files (Node 23 + tsx)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "depositor": "dist/bin.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "tsx src/bin.ts",
    "generate": "tsx src/bin.ts",
    "example:files": "tsx examples/files.ts",
    "example:data": "tsx examples/data.ts",
    "prepublishOnly": "npm run build",
    "build": "tsc --project tsconfig.json",
    "test": "node --import tsx/esm --test test/**/*.ts",
    "test:coverage": "node --import tsx/esm --test --experimental-test-coverage --test-reporter=spec --test-reporter-destination=stdout --test-reporter=lcov --test-reporter-destination=lcov.info test/**/*.ts",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "ethereum",
    "eth2",
    "deposit",
    "keystore",
    "bls",
    "validator"
  ],
  "author": "Yuri Tkachenko",
  "license": "MIT",
  "engines": {
    "node": ">=23"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tamtamchik/depositor.git"
  },
  "bugs": {
    "url": "https://github.com/tamtamchik/depositor/issues"
  },
  "homepage": "https://github.com/tamtamchik/depositor",
  "devDependencies": {
    "@types/node": "^20.11.27",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^8.57.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@chainsafe/bls": "^8.1.0",
    "@chainsafe/bls-keygen": "^0.4.0",
    "@chainsafe/bls-keystore": "^3.1.0",
    "@chainsafe/ssz": "^1.2.0",
    "@lodestar/params": "^1.29.0",
    "@lodestar/state-transition": "^1.29.0",
    "@lodestar/types": "^1.29.0",
    "@scure/bip39": "^1.5.4"
  }
}
